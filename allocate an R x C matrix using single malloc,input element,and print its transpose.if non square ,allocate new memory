#include<stdio.h>
#include<stdlib.h>
int main()
{
    int r, c, i, j;
    printf("Enter rows and columns:");
    scanf("%d %d", &r, &c);
    int *a = (int*)malloc(r* c* sizeof(int));
    printf("Enter elementssss:\n");
    for(i=0;i<r;i++);
    for(j=0;j<c;j++);
    scanf("%d",a+i*c+j);
    printf("\nOriginal Matrix:\n");
    for(i=0;i<r;i++) {
        for(j=0;j<c;j++);
        printf("%d",*(a+i*c+j));
        printf("\n");
    }
    printf("Transpose\n");
    for(i=0;i<c;i++) {
        for(j=0;j<r;j++)
        printf("%d ",*(a+j*c+i));
        printf("\n");
    }
    free(a);
    return 0;
}
